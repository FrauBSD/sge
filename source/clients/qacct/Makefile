QACCT_DIR = $(SDIR)/clients/qacct

QACCT_TARGETS = qacct

QACCT_SRC = qacct.c

QACCT_ADD_SRC = \
            execution_states.c \
            sig_handlers.c

QACCT_OBJS = $(QACCT_SRC:.c=.o) 

QACCT_ADD_OBJS = $(QACCT_ADD_SRC:.c=.o)

QACCT_ADD_LIBS = -lsched -levc -lgdi -lsgeobj -lsgeobjd $(SECLIB) $(KRBLIB) -lcull -lcomm -lcommlists -luti $(WINGRIDLIB) -llck -lrmon $(DLLIB)

QACCT_LIB_DEPENDS = $(EVCLIB) $(GDILIB) $(SGEOBJLIB) $(SGEOBJDLIB) $(KRBLIBS) $(SCHEDLIB) $(CULLLIB) $(COMMLIB) $(COMMLISTSLIB) $(WINGRIDLIB_DEP) $(UTILIB) $(LCKLIB) $(RMONLIB) $(TPLIB)

###############################################################################
# global rules: all, clean, depend, messages
###############################################################################
qacct_clean:
	$(RM) $(QACCT_TARGETS) $(QACCT_OBJS)

qacct_depend:
		$(DEPEND) -f$(QACCT_DIR)/qacct_dependencies $(QACCT_DIR)/*.c

qacct_messages:
	@echo Creating $@
	@for i in `ls $(QACCT_DIR)/*.c`; do \
		$(CC) $(CFLAGS) -DEXTRACT_MESSAGES -E $$i | $(XGETTEXT) - ; \
	done


###############################################################################
# rules to build binaries
###############################################################################
qacct: $(QACCT_OBJS) $(QACCT_ADD_OBJS) $(QACCT_LIB_DEPENDS)
	$(LD_WRAPPER) $(CC) -o $@ $(LFLAGS) $(QACCT_OBJS) $(QACCT_ADD_OBJS) $(QACCT_ADD_LIBS) $(SLIBS) $(LIBS)

###############################################################################
# include dependencies for object files generated by sge_depend
###############################################################################
include $(QACCT_DIR)/qacct_dependencies

###############################################################################
# rules to build object codes
###############################################################################
qacct.o : $(QACCT_DIR)/qacct.c
		$(CC) $(CFLAGS) -c $(QACCT_DIR)/qacct.c
